{
  "name": "seonest-post-gen",
  "version": "0.1.2",
  "keywords": [
    "seonest",
    "markdown",
    "cli",
    "generator",
    "frontmatter"
  ],
  "repository": "https://github.com/JHSeo-git/blog.seonest.net-post-gen.git",
  "license": "MIT",
  "author": "JHSeo <qnfqnfqnf@gmail.com>",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "index.js": "./dist/index.mjs"
      },
      "default": {
        "types": "./dist/index.d.ts",
        "index.js": "./dist/index.mjs"
      }
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "bin": "./dist/index.mjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm clear && rollup -c",
    "clear": "rimraf dist",
    "dts": "tsc --emitDeclarationOnly -p ./tsconfig.build.json",
    "format": "eslint --fix --ext .ts,.js, --ignore-path .gitignore && prettier --write 'src/**/*.{js,ts}'",
    "prepack": "pnpm build",
    "start": "pnpm build && node ./dist/index.mjs",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "chalk": "^5.2.0",
    "cjk-slug": "^0.3.0",
    "commander": "^9.4.1",
    "fs-extra": "^10.1.0",
    "gray-matter": "^4.0.3",
    "inquirer": "^9.1.4"
  },
  "devDependencies": {
    "@rollup/plugin-json": "^4.1.0",
    "@types/fs-extra": "^9.0.13",
    "@types/inquirer": "^9.0.3",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "@vitest/coverage-c8": "^0.23.4",
    "esbuild": "^0.16.13",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-json-format": "^2.0.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "prettier": "^2.8.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.1",
    "rollup-plugin-esbuild": "^5.0.0",
    "rollup-plugin-preserve-shebang": "^1.0.1",
    "typescript": "^4.9.4",
    "vitest": "^0.26.3"
  }
}
